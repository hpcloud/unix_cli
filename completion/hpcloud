_hpcloud()
{
  local cur prev opts cmds
  COMPREPLY=()
  _get_comp_words_by_ref -n : cur prev words
  if [ "${prev}" == "hpcloud" ]
  then
    opts="--help --version"
    cmds="account account:copy account:edit account:remove account:setup account:use account:verify acl acl:grant acl:revoke addresses addresses:add addresses:associate addresses:disassociate addresses:remove cdn:containers cdn:containers:add cdn:containers:get cdn:containers:location cdn:containers:remove cdn:containers:set complete config config:set containers:add containers:remove copy flavors get help images images:add images:metadata images:metadata:add images:metadata:remove images:remove info keypairs keypairs:add keypairs:import keypairs:private keypairs:private:add keypairs:private:location keypairs:private:remove keypairs:public_key keypairs:remove list location move remove securitygroups securitygroups:add securitygroups:remove securitygroups:rules securitygroups:rules:add securitygroups:rules:remove servers servers:add servers:console servers:metadata servers:metadata:add servers:metadata:remove servers:password servers:reboot servers:rebuild servers:remove servers:ssh snapshots snapshots:add snapshots:remove tempurl volumes volumes:add volumes:attach volumes:detach volumes:remove volumes:server"
  else
    cmds=""
    case "${words[1]}" in
    account:edit)
      opts="--no-validate "
      ;;
    account:setup)
      opts="--no-validate "
      ;;
    acl)
      opts="--availability-zone --account-name "
      ;;
    acl:grant)
      opts="--availability-zone --account-name "
      ;;
    acl:revoke)
      opts="--availability-zone --account-name "
      ;;
    addresses)
      opts="--availability-zone --account-name "
      ;;
    addresses:add)
      opts="--availability-zone --account-name "
      ;;
    addresses:associate)
      opts="--availability-zone --account-name "
      ;;
    addresses:disassociate)
      opts="--availability-zone --account-name "
      ;;
    addresses:remove)
      opts="--availability-zone --account-name "
      ;;
    cdn:containers)
      opts="--all --availability-zone --account-name "
      ;;
    cdn:containers:add)
      opts="--availability-zone --account-name "
      ;;
    cdn:containers:get)
      opts="--availability-zone --account-name "
      ;;
    cdn:containers:location)
      opts="--ssl --availability-zone --account-name "
      ;;
    cdn:containers:remove)
      opts="--availability-zone --account-name "
      ;;
    cdn:containers:set)
      opts="--availability-zone --account-name "
      ;;
    containers:add)
      opts="--force --availability-zone --account-name "
      ;;
    containers:remove)
      opts="--force --availability-zone --account-name "
      ;;
    copy)
      opts="--mime --availability-zone --account-name "
      ;;
    flavors)
      opts="--availability-zone --account-name "
      ;;
    get)
      opts="--availability-zone --account-name "
      ;;
    images)
      opts="--availability-zone --account-name "
      ;;
    images:add)
      opts="--availability-zone --account-name --metadata "
      ;;
    images:metadata)
      opts="--availability-zone --account-name "
      ;;
    images:metadata:add)
      opts="--availability-zone --account-name "
      ;;
    images:metadata:remove)
      opts="--availability-zone --account-name "
      ;;
    images:remove)
      opts="--availability-zone --account-name "
      ;;
    keypairs)
      opts="--availability-zone --account-name "
      ;;
    keypairs:add)
      opts="--fingerprint --private-key --output --availability-zone --account-name "
      ;;
    keypairs:import)
      opts="--availability-zone --account-name "
      ;;
    keypairs:public_key)
      opts="--availability-zone --account-name "
      ;;
    keypairs:remove)
      opts="--availability-zone --account-name "
      ;;
    list)
      opts="--availability-zone --account-name "
      ;;
    location)
      opts="--availability-zone --account-name "
      ;;
    move)
      opts="--availability-zone --account-name "
      ;;
    remove)
      opts="--force --availability-zone --account-name "
      ;;
    securitygroups)
      opts="--availability-zone --account-name "
      ;;
    securitygroups:add)
      opts="--availability-zone --account-name "
      ;;
    securitygroups:remove)
      opts="--availability-zone --account-name "
      ;;
    securitygroups:rules)
      opts="--availability-zone --account-name "
      ;;
    securitygroups:rules:add)
      opts="--port-range --cidr --source-group --availability-zone --account-name "
      ;;
    securitygroups:rules:remove)
      opts="--availability-zone --account-name "
      ;;
    servers)
      opts="--availability-zone --account-name "
      ;;
    servers:add)
      opts="--key-name --image --volume --security-group --private-key-file --metadata --availability-zone --account-name "
      ;;
    servers:console)
      opts="--private-key-file --dump-password --availability-zone --account-name "
      ;;
    servers:metadata)
      opts="--availability-zone --account-name "
      ;;
    servers:metadata:add)
      opts="--availability-zone --account-name "
      ;;
    servers:metadata:remove)
      opts="--availability-zone --account-name "
      ;;
    servers:password)
      opts="--availability-zone --account-name "
      ;;
    servers:reboot)
      opts="--hard --availability-zone --account-name "
      ;;
    servers:rebuild)
      opts="--availability-zone --account-name "
      ;;
    servers:remove)
      opts="--availability-zone --account-name "
      ;;
    servers:ssh)
      opts="--private-key-file --keypair --login --command # Default: ssh --availability-zone --account-name "
      ;;
    snapshots)
      opts="--availability-zone --account-name "
      ;;
    snapshots:add)
      opts="--description --metadata --availability-zone --account-name "
      ;;
    snapshots:remove)
      opts="--availability-zone --account-name "
      ;;
    tempurl)
      opts="--time-period --availability-zone --account-name "
      ;;
    volumes)
      opts="--bootable --availability-zone --account-name "
      ;;
    volumes:add)
      opts="--description --metadata --snapshot --image --availability-zone --account-name "
      ;;
    volumes:attach)
      opts="--availability-zone --account-name "
      ;;
    volumes:detach)
      opts="--availability-zone --account-name "
      ;;
    volumes:remove)
      opts="--availability-zone --account-name "
      ;;
    volumes:server)
      opts="--availability-zone --account-name "
      ;;
    esac
  fi
  if [[ ${cur} == -* ]] ; then
    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
    return 0
  else
    if [ -n "${cmds}" ]
    then
      COMPREPLY=( $(compgen -W "${cmds}" -- ${cur}) )
      __ltrim_colon_completions "$cur"
      return 0
    fi
  fi
  return 0
}
complete -F _hpcloud hpcloud
